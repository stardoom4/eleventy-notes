{
  "$schema": ".app/app.schema.json",
  "title": "Celestial Mind",
  "description": "A Digital Garden.",
  "editThisNote": {
    "url": "https://github.com/stardoom4/eleventy-notes/edit/{{branch}}/{{file}}"
  },
  "customProperties": {
    "properties": [
      {
        "path": "props",
        "options": {
          "date": {
            "locale": "en-US"
          }
        }
      }
    ]
  },
  "sidebar": {
    "links": [
      {
        "url": "https://github.com/stardoom4/eleventy-notes",
        "label": "GitHub / Support",
        "icon": "github"
      }
    ],
    "notes": [
      {
        "pattern": "^/[^/]+$"
      },
      {
        "pattern": "^/Wiki/",
        "label": "Wiki",
        "expanded": false,
        "tree": {
          "replace": {
            "^/\\w+": ""
          }
        }
      },
      {
        "pattern": "^/Q/",
        "label": "Question And Answers",
        "expanded": false
      }
        }
      }
    ]
  },
  "tags": {
    "map": {
      "dynamic-content": "dynamic content"
    }
  }
}
